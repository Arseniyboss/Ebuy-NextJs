{
  "name": "ebuy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --watchAll",
    "e2e": "start-server-and-test start 3000 \"cypress open --browser chrome --e2e \"",
    "e2e:headless": "start-server-and-test start 3000 \"cypress run --browser chrome --spec cypress/e2e/${spec=**/*}.cy.ts --e2e\"",
    "compile": "tsc --incremental false",
    "update:latest": "ncu -u && npm install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stripe-webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "trigger-payment-success": "stripe trigger checkout.session.completed"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.1.3",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vercel/kv": "^1.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.0",
    "jose": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "next": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "sharp": "^0.32.6",
    "stripe": "^14.2.0",
    "styled-components": "^6.1.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/jest": "^29.5.6",
    "cypress": "^13.3.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^8.16.0",
    "start-server-and-test": "^2.0.1"
  }
}
