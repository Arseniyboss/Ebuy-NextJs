{
  "name": "ebuy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --watchAll",
    "e2e": "CYPRESS_TEST=true start-server-and-test start 3000 \"cypress open --browser chrome --e2e \"",
    "e2e:headless": "CYPRESS_TEST=true start-server-and-test start 3000 \"cypress run --browser chrome --spec **/*.cy.ts --e2e\"",
    "compile": "tsc --incremental false",
    "update:latest": "ncu -u && npm install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stripe-webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "trigger-payment-success": "stripe trigger checkout.session.completed"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.1.4",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@vercel/kv": "^1.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "jose": "^5.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "next": "14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "sharp": "^0.33.2",
    "stripe": "^14.21.0",
    "styled-components": "^6.1.8",
    "typescript": "5.4.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "cypress": "^13.7.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.7",
    "start-server-and-test": "^2.0.3"
  }
}
